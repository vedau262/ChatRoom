<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_profile"
    app:startDestination="@id/setupFragment">

    <fragment
        android:id="@+id/setupFragment"
        android:name="com.festfive.app.view.chat.SetupFragment"
        android:label="setupFragment"
        tools:layout="@layout/fragment_setup">
        <action
            android:id="@+id/action_setupFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_setupFragment_to_videoCallFragment"
            app:destination="@id/videoCallFragment" />
        <action
            android:id="@+id/action_setupFragment_to_groupVideoCallFragment"
            app:destination="@id/groupVideoCallFragment" />
    </fragment>

    <fragment
        android:id="@+id/chatFragment"
        android:name="com.festfive.app.view.chat.ChatFragment"
        android:label="SelectGenderFragment"
        tools:layout="@layout/fragment_chat"/>
    <fragment
        android:id="@+id/videoCallFragment"
        android:name="com.festfive.app.view.chat.VideoCallFragment"
        android:label="VideoCallFragment"
        tools:layout="@layout/fragment_video_call"/>
    <fragment
        android:id="@+id/groupVideoCallFragment"
        android:name="com.festfive.app.view.chat.GroupVideoCallFragment"
        android:label="GroupVideoCallFragment" />
</navigation>